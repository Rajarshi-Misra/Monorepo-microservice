{
  "name": "mynth",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm --workspace services/service-a run dev\" \"npm --workspace services/service-b run dev\"",
    "build": "npm --workspace common run build && npm --workspace services/service-a run build && npm --workspace services/service-b run build",
    "lint": "eslint . --ext .ts,.js --ignore-pattern .gitignore",
    "format": "prettier --write .",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration", 
    "test:e2e": "jest --testPathPattern=e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:common": "npm --workspace common test",
    "test:service-a": "npm --workspace services/service-a test",
    "test:service-b": "npm --workspace services/service-b test",
    "test:all": "npm run test:common && npm run test:service-a && npm run test:service-b && npm run test:e2e"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "private": true,
  "workspaces": [
    "services/service-a",
    "services/service-b",
    "common"
  ],
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@types/amqplib": "^0.10.7",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
